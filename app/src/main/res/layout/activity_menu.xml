<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.example.nicolai.arbeigobetalingsserivce.MenuActivity">



    <ImageView
        android:id="@+id/ArbeigoHeader"
        android:layout_width="wrap_content"
        android:layout_height="119dp"
        android:src="@drawable/headerarbeigo"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />


    <ImageView
        android:id="@+id/ArbeigoUser"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/ArbeigoHeader"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/user"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="158dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="330dp"
        android:layout_height="45dp"
        android:layout_alignTop="@+id/ArbeigoUser"
        android:layout_marginLeft="7dp"
        android:layout_marginStart="7dp"
        android:layout_toEndOf="@+id/ArbeigoUser"
        android:layout_toRightOf="@+id/ArbeigoUser"
        android:background="@color/white"
        android:text="Min bruger"
        android:textColor="@android:color/black" />

</RelativeLayout>
